#include<stdio.h>
int main()
{
    int a,b,i,gcd,sd1,sd2;
    printf("Name:-Mangesh Roll NO :- FIA23\nEnter two numbers:");
    scanf("%d%d",&a,&b);
    for(i=2;i<=a;i++)
    {
        if(a%i==0)
        {
            sd1=i;
            break;
        }
    }
    for(i=2;i<=b;i++)
    {
        if(b%i==0)
        {
            sd2=i;
            break;
        }
    }
    for(i=1;i<=a&&i<=b;i++)
    {
        if(a%i==0&&b%i==0)
        gcd=i;
    }
    printf("Smallest divisor of %d=%d\n",a,sd1);
    printf("Smallest divisor of %d=%d\n",b,sd2);
    printf("GCD of %d and %d=%d\n",a,b,gcd);
    return 0;
}
