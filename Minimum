#include <stdio.h>
void grade(float per);
float percentage(int total);
int pass(int Sub1, int Sub2, int Sub3, int Sub4, int Sub5);
// Main Function
int main()
{
 int Sub1, Sub2, Sub3, Sub4, Sub5, total;
 float per;
 printf(" code by:pranav vyawhare roll no:58\n Enter marks of 5 subjects:\n");
 scanf("%d %d %d %d %d", &Sub1, &Sub2, &Sub3, &Sub4, &Sub5);

// Check pass or fail
 if (pass(Sub1, Sub2, Sub3, Sub4, Sub5))
 {
 total =Sub1 + Sub2 + Sub3 + Sub4 + Sub5;
 per = percentage(total);
 printf("Percentage = %.2f\n", per);
 grade(per);
 }
else
 {
 printf("Student Failed (Marks below 40 in any subject)\n");
 }
 return 0;
 }
// Function to calculate percentage
float percentage(int total)
{
 return (total / 500.0) * 100;
}
// Function to check if pass
int pass(int Sub1, int Sub2, int Sub3, int Sub4, int Sub5)
{
 if (Sub1 < 40 || Sub2 < 40 || Sub3 < 40 || Sub4 < 40 || Sub5 < 40)
 return 0; // fail
 else
 return 1; // pass
}
// Function to print grade
void grade(float per)
 {
 if (per > 75)
 printf("Grade: Distinction\n");

else if (per >= 60)
 printf("Grade: First Division\n");

else if (per >= 50)
 printf("Grade: Second Division\n");

else if (per >= 40)
 printf("Grade: Third Division\n");

 else
 printf("Grade: Fail\n");
 }
