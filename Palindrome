#include <stdio.h>
#include <string.h>

int main() {
    char str[100], rev[100];

    printf("name: Mangesh , roll no : 23\n");
    printf("Enter a string: ");
    scanf("%s", str);

    strcpy(rev, str);     
    strrev(rev);        

    if (strcmp(str, rev) == 0)
        printf("It is a Palindrome String.\n");
    else
        printf("It is NOT a Palindrome String.\n");

    return 0;
}
