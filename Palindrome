#include <stdio.h>
#include <string.h>

void reverseString(char str[], char rev[]) {
    int len = strlen(str);
    for (int i = 0; i < len; i++) {
        rev[i] = str[len - i - 1];
    }
    rev[len] = '\0';  // Null terminate the reversed string
}

int main() {
    char str[100], rev[100];
    printf("Enter a string: ");
    scanf("%s", str);

    reverseString(str, rev);

    if (strcmp(str, rev) == 0)
        printf("It is a Palindrome String.\n");
    else
        printf("It is NOT a Palindrome String.\n");

    return 0;
}
